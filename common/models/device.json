{
  "name": "Device",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "scans": {
      "type": "hasMany",
      "model": "Scan",
      "foreignKey": ""
    },
    "currentLocation": {
      "type": "belongsTo",
      "model": "Location",
      "foreignKey": "currentLocationId"
    },
    "locationsHistory": {
      "type": "hasMany",
      "model": "TimedLocation",
      "foreignKey": "deviceId"
    },
    "currentCoordinate": {
      "type": "belongsTo",
      "model": "Coordinate",
      "foreignKey": "currentCoordinateId"
    },
    "coordinatesHistory": {
      "type": "hasMany",
      "model": "Coordinate",
      "foreignKey": "deviceId"
    },
    "rssiTransformations": {
      "type": "hasMany",
      "model": "RssiTransformation",
      "foreignKey": "deviceId",
      "through": "FingerprintTransformation"
    },
    "fingerprints": {
      "type": "hasMany",
      "model": "Fingerprint",
      "foreignKey": "deviceId",
      "through": "FingerprintTransformation"
    }
  },
  "acls": [],
  "methods": {}
}
